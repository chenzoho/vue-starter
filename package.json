{
  "name": "vue-admin-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "lint:js": "eslint ./src --ext .vue,.js,.ts",
    "lint:js-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "lint:css": "stylelint ./src/**/*.{css,less,sass,scss,vue}",
    "lint:css-fix": "stylelint ./src/**/*.{css,less,sass,scss,vue} --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@vueuse/core": "^7.7.1",
    "axios": "^0.26.1",
    "element-plus": "^2.2.5",
    "js-base64": "^3.7.2",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.0.14",
    "qs": "^6.10.5",
    "screenfull": "^6.0.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vxe-table": "^4.2.3",
    "xe-utils": "^3.5.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.42",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@types/qs": "^6.9.7",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^6.8.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "sass": "^1.52.3",
    "stylelint": "^14.9.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,md,json,vue}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,less,sass,scss,vue}": [
      "stylelint --fix",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
